    // Example of Adding configs for VS Code
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Speak Example",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}\\cmd\\examples\\speak\\main.go",
            "env": {
                "BOT_TARGET":"YOUR_VECTOR_IP_HERE:443",
                "BOT_TOKEN":"YOUR SESSION TOKEN GUID HERE",
            },
        },
        {
            "name": "Escape-Pod Example",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}\\cmd\\escape-pod-login\\main.go",
            "args": [
                "-host",
                "YOUR_VECTOR_IP_HERE"
            ],
        }
    ]
}